function upper_lower(string){
    ns = '';
    for(let i=0; i<string.length; i++){
        if(string[i].charCodeAt() >= 97 && string[i].charCodeAt() <= 122){
            ns += string.charAt(i).toUpperCase();
        }
        else    
            ns += string.charAt(i).toLowerCase();
    }
    return ns;
    
}
console.log(upper_lower("BhavAna"));
