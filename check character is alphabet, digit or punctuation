function check(c) {
    if ((c.charCodeAt() >= 97 && c.charCodeAt() <= 122) || (c.charCodeAt() >= 65 && c.charCodeAt() <= 90))
        return ('it is alphabet');
    else if (c.charCodeAt() >= 48 && c.charCodeAt() <= 57)
        return ('its a digit');
    else
        return ('its a special character');
}
console.log(check("8"));
