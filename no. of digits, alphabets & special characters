var s = "bhask4df9542##"
var str = s.trim();
var d = 0;
var a = 0;
var s = 0;
for(let i=0; i<str.length; i++){
    if(str[i] >= '0' && str[i] <= '9')
        d++;
    else if(str[i] >= 'a' && str[i] <= 'z' || str[i] >= 'A' && str[i] <= 'Z')
        a++;
    else
        s++;
}   
console.log(d,a,s);
